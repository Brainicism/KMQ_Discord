{
  "name": "kmq",
  "version": "3.10.1",
  "main": "src/kmq.js",
  "scripts": {
    "seed": "ts-node src/seed/seed_db",
    "bootstrap": "ts-node src/seed/bootstrap.ts",
    "start": "./start.sh native",
    "dry-run": "NODE_ENV=dry-run npm run start",
    "dev": "NODE_ENV=development npm run start",
    "ci": "NODE_ENV=ci npm run start",
    "prod": "NODE_ENV=production npm run start",
    "clean": "rm -rf build/ && rm -f tsconfig.tsbuildinfo",
    "download_songs": "ts-node src/scripts/download-new-songs.ts",
    "lint": "npx eslint --cache . --ext .js,.ts",
    "lint_fix": "npx eslint --fix --cache . --ext .js,.ts",
    "lint_ci": "npx eslint --cache . --ext .js,.ts --max-warnings=0",
    "docker-db": "cd docker/ && docker-compose up -d db",
    "docker-dev": "cd docker/ && NODE_ENV=development docker-compose up  --exit-code-from kmq --build kmq",
    "docker-dryrun": "cd docker/ && NODE_ENV=dry-run docker-compose up --exit-code-from kmq --build kmq",
    "docker-prod": "cd docker/ && NODE_ENV=production docker-compose up --exit-code-from kmq --build kmq",
    "backup": "ts-node src/scripts/backup-kmq-database",
    "import": "ts-node src/scripts/backup-kmq-database --import",
    "test": "NODE_ENV=test mocha -r ts-node/register 'src/test/**/*.ts'",
    "test_ci": "NODE_ENV=test mocha -- --forbid-only -r ts-node/register 'src/test/**/*.ts'",
    "prettier": "prettier --write '**/*.{js,ts}'",
    "prettier_ci": "prettier --check '**/*.{js,ts}'"
  },
  "author": "Brian Le",
  "license": "GPL-3.0",
  "dependencies": {
    "@discordjs/opus": "^0.5.3",
    "@types/damerau-levenshtein": "^1.0.0",
    "@types/lodash": "^4.14.170",
    "@types/mocha": "^8.2.1",
    "@types/node-schedule": "^1.3.1",
    "@types/pluralize": "^0.0.29",
    "@types/sinon": "^10.0.2",
    "axios": "^0.21.3",
    "bufferutil": "^4.0.3",
    "commander": "^7.0.0",
    "cron-parser": "^3.1.0",
    "damerau-levenshtein": "^1.0.7",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "eris": "https://github.com/Brainicism/eris.git#d46b865",
    "eris-fleet": "^1.0.0",
    "eris-pagination": "https://github.com/brainicism/eris-pagination#a30b37b",
    "erlpack": "https://github.com/abalabahaha/erlpack.git",
    "fastify": "^3.17.0",
    "fastify-response-caching": "https://github.com/TheTimmaeh/fastify-response-caching#d71df88",
    "fluent-ffmpeg": "^2.1.2",
    "knex": "^0.95.6",
    "lodash": "^4.17.21",
    "mocha": "^9.0.0",
    "moment-timezone": "^0.5.32",
    "mysql2": "^2.2.5",
    "node-schedule": "^2.0.0",
    "pluralize": "^8.0.0",
    "point-of-view": "^4.15.2",
    "sinon": "^11.1.1",
    "sodium-native": "^3.2.1",
    "ts-node": "^10.0.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5",
    "ytdl-core": "^4.9.2",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-tsdoc": "^0.2.14",
    "prettier": "2.5.0",
    "typescript": "~4.4.2"
  },
  "description": "K-pop guessing game over Discord"
}
