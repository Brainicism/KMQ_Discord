{
  "name": "kmq",
  "version": "1.0.0",
  "main": "src/kmq.js",
  "scripts": {
    "seed": "ts-node src/seed/seed_db",
    "bootstrap": "ts-node src/seed/bootstrap.ts",
    "start": "./start.sh native",
    "dry-run": "NODE_ENV=dry-run npm run start",
    "dev": "NODE_ENV=development npm run start",
    "ci": "NODE_ENV=ci npm run start",
    "prod": "NODE_ENV=production npm run start",
    "clean": "rm -rf build/ && rm -f tsconfig.tsbuildinfo",
    "download_songs": "ts-node src/scripts/download-new-songs.ts",
    "lint": "npx eslint . --ext .js,.ts",
    "lint_ci": "npx eslint . --ext .js,.ts --max-warnings=0",
    "docker-db": "cd docker/ && docker-compose up -d db",
    "docker-dev": "cd docker/ && NODE_ENV=development docker-compose up  --exit-code-from kmq --build kmq",
    "docker-dryrun": "cd docker/ && NODE_ENV=dry-run docker-compose up --exit-code-from kmq --build kmq",
    "docker-prod": "cd docker/ && NODE_ENV=production docker-compose up --exit-code-from kmq --build kmq",
    "backup": "ts-node src/scripts/backup-kmq-database",
    "test": "NODE_ENV=test mocha -r ts-node/register 'src/test/**/*.ts'",
    "test_ci": "NODE_ENV=test mocha -- --forbid-only -r ts-node/register 'src/test/**/*.ts'"
  },
  "author": "Brian Le",
  "license": "GPL-3.0",
  "dependencies": {
    "@discordjs/opus": "^0.5.3",
    "@types/lodash": "^4.14.170",
    "@types/node-schedule": "^1.3.1",
    "@types/sinon": "^10.0.2",
    "@types/mocha": "^8.2.1",
    "axios": "^0.21.1",
    "bufferutil": "^4.0.3",
    "commander": "^7.0.0",
    "cron-parser": "^3.1.0",
    "dotenv": "^10.0.0",
    "eris": "https://github.com/Brainicism/eris.git#2c15eab",
    "eris-fleet": "^0.3.8",
    "eris-pagination": "^0.5.2",
    "erlpack": "https://github.com/abalabahaha/erlpack.git#5d0064f",
    "fastify": "^3.17.0",
    "fluent-ffmpeg": "^2.1.2",
    "knex": "^0.95.6",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "mocha": "^9.0.0",
    "moment-timezone": "^0.5.32",
    "mysql2": "^2.2.5",
    "mysqldump": "^3.2.0",
    "node-schedule": "^2.0.0",
    "sinon": "^11.1.1",
    "sodium-native": "^3.2.1",
    "ts-node": "^10.0.0",
    "uuid": "^8.3.2",
    "ytdl-core": "^4.8.3",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-tsdoc": "^0.2.11",
    "typescript": "~4.3.2"
  },
  "description": "K-pop guessing game over Discord"
}
