{
  "name": "kmq",
  "version": "1.0.0",
  "main": "src/kmq.js",
  "scripts": {
    "seed": "ts-node src/seed/seed_db",
    "start": "./start.sh",
    "dry-run": "npm run lint && START_TYPE=dry-run npm run start",
    "dev": "npm run lint && START_TYPE=dev npm run start",
    "dev-no-lint": "START_TYPE=dev npm run start",
    "prod": "npm run lint && START_TYPE=prod npm run start",
    "clean": "rm -rf build/ && rm -f tsconfig.tsbuildinfo",
    "bootstrap": "ts-node src/seed/bootstrap.ts",
    "download_songs": "ts-node src/scripts/download-new-songs.ts",
    "lint": "npx eslint . --ext .js,.ts",
    "lint_ci": "npx eslint . --ext .js,.ts --max-warnings=0",
    "docker-db": "cd docker/ && docker-compose up -d db",
    "docker-dev": "cd docker/ && START_TYPE=dev docker-compose up --build kmq",
    "docker-prod": "cd docker/ && START_TYPE=prod docker-compose up --build kmq",
    "backup": "ts-node src/scripts/backup-kmq-database",
    "import": "ts-node src/scripts/backup-kmq-database --import",
    "test": "NODE_ENV=test mocha -r ts-node/register 'src/test/**/*.ts'",
    "test_ci": "NODE_ENV=test mocha -- --forbid-only -r ts-node/register 'src/test/**/*.ts'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/opus": "^0.5.0",
    "@types/node-schedule": "^1.3.1",
    "axios": "^0.21.1",
    "bufferutil": "^4.0.3",
    "commander": "^7.0.0",
    "cron-parser": "^3.1.0",
    "dotenv": "^8.2.0",
    "eris": "https://github.com/Brainicism/eris.git#aa344e5",
    "eris-pagination": "^0.5.2",
    "erlpack": "https://github.com/abalabahaha/erlpack.git#5d0064f",
    "fluent-ffmpeg": "^2.1.2",
    "glob": "^7.1.6",
    "knex": "^0.21.19",
    "log4js": "^6.3.0",
    "moment-timezone": "^0.5.32",
    "mysql": "^2.18.1",
    "node-schedule": "^1.3.3",
    "node-stream-zip": "^1.13.2",
    "sodium-native": "^3.2.1",
    "uuid": "^8.3.2",
    "ytdl-core": "^4.4.5",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.1",
    "@types/sinon": "^9.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-tsdoc": "^0.2.11",
    "mocha": "^8.3.1",
    "sinon": "^9.2.4",
    "ts-node-dev": "^1.1.1",
    "typescript": "~4.0.5"
  },
  "description": ""
}
