{
    "name": "kmq",
    "version": "5.5.1",
    "main": "src/kmq.js",
    "scripts": {
        "seed": "ts-node src/seed/seed_db",
        "bootstrap": "ts-node src/seed/bootstrap.ts",
        "start": "./start.sh native",
        "dry-run": "NODE_ENV=dry-run npm run start",
        "dev": "NODE_ENV=development npm run start",
        "ci": "NODE_ENV=ci npm run start",
        "prod": "NODE_ENV=production npm run start",
        "clean": "rm -rf build/ && rm -f tsconfig.tsbuildinfo",
        "download_songs": "ts-node src/scripts/download-new-songs.ts",
        "lint": "npx eslint --cache src/ --ext .js,.ts",
        "lint_i18n": "npx eslint --no-eslintrc --config .eslintrc.i18n.js --fix --ext .json --format node_modules/eslint-plugin-i18n-json/formatter.js i18n/ ",
        "lint_i18n_ci": "npx eslint --no-eslintrc --config .eslintrc.i18n.js --ext .json  --max-warnings=0 --format node_modules/eslint-plugin-i18n-json/formatter.js i18n/ ",
        "lint_fix": "npx eslint --fix --cache src/ --ext .js,.ts",
        "lint_ci": "npx eslint --cache src/ --ext .js,.ts --max-warnings=0",
        "docker-db": "cd docker/ && docker-compose up -d db",
        "docker-dev": "cd docker/ && NODE_ENV=development docker-compose up  --exit-code-from kmq --build kmq",
        "docker-dryrun": "cd docker/ && NODE_ENV=dry-run docker-compose up --exit-code-from kmq --build kmq",
        "docker-prod": "cd docker/ && NODE_ENV=production docker-compose up --exit-code-from kmq --build kmq",
        "backup": "ts-node src/scripts/backup-kmq-database",
        "import": "ts-node src/scripts/backup-kmq-database --import",
        "test": "NODE_ENV=test mocha -r ts-node/register 'src/test/**/*.ts'",
        "test_ci": "NODE_ENV=test mocha -- --forbid-only -r ts-node/register 'src/test/**/*.ts'",
        "prettier": "prettier --write '**/*.{js,ts}'",
        "prettier_ci": "prettier --check '**/*.{js,ts}'"
    },
    "author": "Brian Le",
    "license": "GPL-3.0",
    "dependencies": {
        "@discordjs/opus": "^0.7.0",
        "@types/damerau-levenshtein": "^1.0.0",
        "@types/i18next-fs-backend": "^1.1.2",
        "@types/lodash": "^4.14.178",
        "@types/mocha": "^9.1.0",
        "@types/node-schedule": "^1.3.2",
        "@types/pluralize": "^0.0.29",
        "@types/sinon": "^10.0.10",
        "axios": "^0.26.1",
        "bufferutil": "^4.0.6",
        "commander": "^9.0.0",
        "cron-parser": "^4.2.1",
        "damerau-levenshtein": "^1.0.8",
        "dotenv": "^16.0.0",
        "ejs": "^3.1.6",
        "eris": "https://github.com/Brainicism/eris.git#fce3f48",
        "eris-fleet": "^1.0.0",
        "eris-pagination": "https://github.com/brainicism/eris-pagination#a30b37b",
        "erlpack": "https://github.com/abalabahaha/erlpack.git",
        "fastify": "^3.27.1",
        "fastify-response-caching": "https://github.com/TheTimmaeh/fastify-response-caching.git",
        "fluent-ffmpeg": "^2.1.2",
        "i18next": "^21.6.10",
        "i18next-fs-backend": "^1.1.4",
        "knex": "^2.0.0",
        "lodash": "^4.17.21",
        "mocha": "^9.2.0",
        "moment-timezone": "^0.5.34",
        "mysql2": "^2.3.3",
        "node-schedule": "^2.1.0",
        "point-of-view": "^5.1.0",
        "sinon": "^13.0.1",
        "sodium-native": "^3.3.0",
        "ts-node": "^10.4.0",
        "uuid": "^8.3.2",
        "winston": "^3.5.1",
        "winston-daily-rotate-file": "^4.6.0",
        "ytdl-core": "^4.10.1",
        "zlib-sync": "^0.1.7"
    },
    "devDependencies": {
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.10.2",
        "@typescript-eslint/parser": "^5.10.2",
        "eslint": "^8.13.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-deprecation": "^1.3.2",
        "eslint-plugin-i18n-json": "https://github.com/godaddy/eslint-plugin-i18n-json#13d5f18",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsdoc": "^39.2.7",
        "eslint-plugin-only-warn": "^1.0.3",
        "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
        "eslint-plugin-tsdoc": "^0.2.14",
        "prettier": "2.6.2",
        "typescript": "~4.6.3"
    },
    "description": "K-pop guessing game over Discord"
}
