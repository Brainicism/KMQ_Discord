<script>
  async function displayClusterInfo() {
    const guildID = prompt("What's your server ID?");
    if (!guildID) return;
    if (isNaN(guildID)) return;

    const shardCount = <%= overallStatsData.shardCount %>;

    // https://discord.com/developers/docs/topics/gateway#sharding
    const allocatedShard = parseInt(BigInt(guildID) >> BigInt(22)) % shardCount;
    alert(`Your server is on shard: ${allocatedShard}`);
  }
</script>
