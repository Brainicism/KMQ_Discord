    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch KMQ",
            "runtimeArgs": ["-r", "@swc-node/register"],
            "args": ["${workspaceFolder}/src/kmq.ts"],
            "envFile": "${workspaceFolder}/.env",
            "outputCapture": "std",
            "env": {
                "NODE_ENV": "development_ts_node"
            }
        }
    ]
